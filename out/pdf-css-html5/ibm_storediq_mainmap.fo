                                <fo:block-container font-size="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1.5em" page-break-after="avoid" id="ariaid-title10"><fo:block end-indent="0" start-indent="0" role="H2">Open
                                    Virtual Appliance (OVA) configuration requirements</fo:block></fo:block-container>
                                
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">IBM StoredIQ is
                                        deployed as virtual appliances and is supported in VMware
                                        ESXi 5.0 (all fix pack levels) or VMware ESXi 6.0 (all fix
                                        pack levels) environments. You must have a virtual
                                        infrastructure that meets the IBM StoredIQ hardware
                                        requirements.</fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e2822" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Application
                                            stack</fo:block></fo:block-container>
                                        <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em">
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>vCPU: 4</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Memory: 8 GB</fo:block></fo:list-item-body></fo:list-item>
