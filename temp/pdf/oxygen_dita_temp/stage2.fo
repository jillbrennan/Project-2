                        <fo:table-row keep-together.within-page="always">
                            <fo:table-cell text-align="from-table-column()" border-after-style="solid" border-after-width="1pt" border-after-color="black" border-after-width.conditionality="retain" border-before-style="solid" border-before-width="1pt" border-before-color="black" border-end-style="solid" border-end-width="1pt" border-end-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">8765</fo:block></fo:table-cell>
                        </fo:table-row>
                        <fo:table-row keep-together.within-page="always">
                            <fo:table-cell text-align="from-table-column()" border-after-style="solid" border-after-width="1pt" border-after-color="black" border-after-width.conditionality="retain" border-before-style="solid" border-before-width="1pt" border-before-color="black" border-end-style="solid" border-end-width="1pt" border-end-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">7766</fo:block></fo:table-cell>      
                        </fo:table-row>
                        <fo:table-row keep-together.within-page="always">
